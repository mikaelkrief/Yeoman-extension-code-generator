{
  "name": "generator-team-services-extension",
  "version": "1.0.51",
  "description": "Yeoman generator for Team Services and Team Foundation Server extensions",
  "homepage": "https://github.com/ALM-Rangers/generator-team-services-extension",
  "author": {
    "name": "Visual Studio ALM Rangers"
  },
  "files": [
    "generators/app",
    "generators/hub",
    "generators/taskitem",
    "generators/widget",
    "generators/servicehook"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Visual Studio Team Services",
    "Team Foundation Server",
    "extension",
    "task",
    "widget",
    "service hook",
    "yeoman-generator",
    "application insight"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ALM-Rangers/generator-team-services-extension"
  },
  "dependencies": {
    "yeoman-generator": "^0.24.1",
    "chalk": "^1.0.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "yeoman-test": "*",
    "yeoman-assert": "*",
    "eslint": "^3.1.1",
    "eslint-config-xo-space": "^0.15.0",
    "jshint": "^2.9.4",
    "uuid": "*",
    "fs-extra": "^0.30.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "sinon": "*",
    "typescript": "^2.1.4",
    "yo": "^1.8.5",
    "xunit-file": "^1.0.0",
    "snyk": "^1.24.6",
    "gulp": "^3.9.1",
    "gulp-inline-source": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "license": "MIT",
  "scripts": {
    "test": "istanbul cover --report cobertura --report html node_modules/mocha/bin/_mocha -- -R xunit-file && npm run inlinesource",
    "snyk:test": "snyk test",
    "inlinesource": "gulp inlinesource"
  }
}
